      "      },",
      "      page: {",
      "        pageSize: 10,",
      "        currentPage: 1,",
      "        total: 0",
      "      },",
      "      query: {},",
      "      data: [],",
      "      selectionList: [],",
      "    }",
      "  },",
      "  computed: {},",
      "  mounted() {",
