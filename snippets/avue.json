  "avue-diy-edit-view": {
    "prefix": "adev",
    "body": [
      "<template>",
      "  <basic-container>",
      "    <avue-crud",
      "      :option=\"option\"",
      "      :table-loading=\"loading\"",
      "      :data=\"data\"",
      "      :page.sync=\"page\"",
      "      ref=\"crud\"",
      "      @search-change=\"searchChange\"",
      "      @search-reset=\"searchReset\"",
