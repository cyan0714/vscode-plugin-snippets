      "            this.formData.fileList.forEach( item => {",
      "              item.fileType = 1 // 验房文件类型为1",
      "            })",
      "          }",
      "          let params = {",
      "            ...this.formData",
      "          }",
      "          addOrEdit(params).then(() => {",
      "              this.onLoad(this.page);",
      "              this.\\$message({",
      "                type: \"success\",",
      "                message: \"操作成功!\"",
      "              });",
      "              done()",
      "              this.dialogVisible = false",
      "              this.\\$refs.formData.clearValidate()",
      "          }, error => {",
      "              window.console.log(error);",
      "              done();",
      "          })",
      "        }",
      "      })",
      "    },",
      "    rowDel(form, index) {",
