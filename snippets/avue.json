      "import { getList, remove, detail, addOrEdit } from '@/api/info/${TM_FILENAME_BASE}';",
      "export default {",
      "  data() {",
      "    return {",
      "      dialogTitle: '修改',",
      "      dialogVisible: false,",
      "      formData: {},",
      "      formDataOption: {",
      "        submitBtn: false,",
      "        emptyBtn: false,",
      "        span: 12,",
